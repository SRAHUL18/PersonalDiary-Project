<%- include("partials/header"); -%> 
<h1>Compose</h1>
<form action="compose" method="post" autocomplete="off">
    <div class="form-group">
        <label>Title</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" name="postTitle">
      </div>

      <div class="form-group">
        <label>Post</label>
        <textarea class="form-control" id="exampleFormControlTextarea1" rows="3" name="postBody"></textarea>
      </div>
    

<button class="btn btn-primary" type="submit" >Publish</button>

</form>



